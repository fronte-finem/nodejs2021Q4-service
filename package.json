{
  "name": "nodejs-2021q4-service",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "type": "module",
  "description": "REST service â€” education project from RS School for course Nodejs 2021Q4",
  "main": "src/server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon src/server.js",
    "lint": "eslint --ext .js,.ts --fix src/",
    "test": "cross-env DEBUG=rs:* jest --testMatch \"<rootDir>/test/e2e/test/*.test.js\" --noStackTrace --runInBand",
    "test:auth": "cross-env DEBUG=rs:* TEST_MODE=auth jest --noStackTrace"
  },
  "keywords": [
    "rs",
    "school",
    "rest",
    "node",
    "express",
    "autotest",
    "starter"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0 <17"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "@mgcrea/fastify-request-logger": "^0.5.2",
    "@mgcrea/pino-pretty-compact": "^0.5.0",
